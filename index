<script>
function getLines(id){
  const raw = document.getElementById(id).textContent;
  return raw.replace(/\r/g,"\n").split(/\n/);
}
function typer(el, lines, opts = {}){
  const speed = opts.speed ?? 14;
  const lineDelay = opts.lineDelay ?? 240;
  const prompt = `<span class="prompt">$</span> `;
  let i = 0, j = 0, active = true;
  const cursor = document.createElement("span");
  cursor.className = "cursor";
  cursor.setAttribute("aria-hidden","true");
  function typeNext(){
    if(!active) return;
    if(i >= lines.length){ el.appendChild(cursor); return; }
    const line = lines[i] ?? "";
    const lineEl = document.createElement("div");
    if(/^\s*$/.test(line) || /^\W*\//.test(line) || /^(fragments from unstable threads)/i.test(line)){
      lineEl.textContent = line;
      el.appendChild(lineEl);
      i++; j=0;
      setTimeout(typeNext, lineDelay);
      return;
    }
    lineEl.innerHTML = prompt;
    const span = document.createElement("span");
    lineEl.appendChild(span);
    el.appendChild(lineEl);
    (function tick(){
      if(!active) return;
      if(j < line.length){
        span.textContent += line.charAt(j++);
        setTimeout(tick, speed);
      }else{
        i++; j=0;
        setTimeout(typeNext, lineDelay);
      }
    })();
  }
  el.addEventListener('click', ()=>{ active = !active; if(active) typeNext(); }, {passive:true});
  typeNext();
}
function bootChime(){
  try{
    if (window.matchMedia && window.matchMedia('(prefers-reduced-motion: reduce)').matches) return;
    const ctx = new (window.AudioContext || window.webkitAudioContext)();
    const master = ctx.createGain();
    master.gain.value = 0.2;
    master.connect(ctx.destination);
    const notes = [98, 147, 196, 233, 311, 392];
    const now = ctx.currentTime;
    notes.forEach((f, idx)=>{
      const o = ctx.createOscillator();
      const g = ctx.createGain();
      const filt = ctx.createBiquadFilter();
      o.type = 'sawtooth';
      o.frequency.setValueAtTime(f, now);
      filt.type = 'lowpass';
      filt.frequency.setValueAtTime(900 + idx*120, now + idx*0.035);
      g.gain.setValueAtTime(0.0001, now + idx*0.035);
      g.gain.exponentialRampToValueAtTime(0.22, now + idx*0.035 + 0.02);
      g.gain.exponentialRampToValueAtTime(0.0001, now + idx*0.035 + 0.42);
      o.connect(filt); filt.connect(g); g.connect(master);
      o.start(now + idx*0.035);
      o.stop(now + idx*0.035 + 0.44);
    });
    const bufferSize = 0.35 * ctx.sampleRate;
    const buffer = ctx.createBuffer(1, bufferSize, ctx.sampleRate);
    const data = buffer.getChannelData(0);
    for (let i=0;i<bufferSize;i++){ data[i] = (Math.random()*2-1) * (1 - i/bufferSize); }
    const noise = ctx.createBufferSource();
    noise.buffer = buffer;
    const nFilter = ctx.createBiquadFilter(); nFilter.type='bandpass'; nFilter.frequency.value=1300; nFilter.Q.value=5;
    const nGain = ctx.createGain();
    nGain.gain.setValueAtTime(0.0001, now + 0.12);
    nGain.gain.exponentialRampToValueAtTime(0.18, now + 0.16);
    nGain.gain.exponentialRampToValueAtTime(0.0001, now + 0.5);
    noise.connect(nFilter); nFilter.connect(nGain); nGain.connect(master);
    noise.start(now + 0.12); noise.stop(now + 0.52);
  }catch(e){}
}
window.addEventListener('DOMContentLoaded', ()=>{
  document.getElementById('y').textContent = new Date().getFullYear();
  const leftLines = getLines('constants-text');
  const rightLines = getLines('variables-text');
  typer(document.getElementById('screen-constants'), leftLines, {speed: 14, lineDelay: 240});
  typer(document.getElementById('screen-variables'), rightLines, {speed: 14, lineDelay: 240});
  bootChime();
});
</script>
